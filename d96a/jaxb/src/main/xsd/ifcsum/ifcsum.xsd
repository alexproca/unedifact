      <xsd:element form="qualified" maxOccurs="9" minOccurs="0" name="CUX" type="Q1:Cux"/>
      <xsd:element form="qualified" maxOccurs="99" minOccurs="0" name="MOA" type="Q1:Moa"/>
      <xsd:element form="qualified" maxOccurs="99" minOccurs="0" name="FTX" type="Q1:Ftx"/>
      <xsd:element form="qualified" maxOccurs="99" minOccurs="0" name="Segment_group_20" type="ifcsum:SegmentGroup20"/>
      <xsd:element form="qualified" maxOccurs="2" minOccurs="0" name="Segment_group_21" type="ifcsum:SegmentGroup21"/>
      <xsd:element form="qualified" maxOccurs="99" minOccurs="0" name="Segment_group_22" type="ifcsum:SegmentGroup22"/>
      <xsd:element form="qualified" maxOccurs="9" minOccurs="0" name="Segment_group_23" type="ifcsum:SegmentGroup23"/>
      <xsd:element form="qualified" maxOccurs="9" minOccurs="0" name="Segment_group_25" type="ifcsum:SegmentGroup25"/>
      <xsd:element form="qualified" maxOccurs="99" minOccurs="0" name="Segment_group_26" type="ifcsum:SegmentGroup26"/>
      <xsd:element form="qualified" maxOccurs="99" minOccurs="0" name="Segment_group_27" type="ifcsum:SegmentGroup27"/>
      <xsd:element form="qualified" maxOccurs="99" minOccurs="0" name="Segment_group_30" type="ifcsum:SegmentGroup30"/>
      <xsd:element form="qualified" maxOccurs="999" minOccurs="0" name="Segment_group_37" type="ifcsum:SegmentGroup37"/>
      <xsd:element form="qualified" maxOccurs="999" minOccurs="0" name="Segment_group_56" type="ifcsum:SegmentGroup56"/>
    </xsd:sequence>
  </xsd:complexType>
